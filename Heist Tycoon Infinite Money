local LocalPlayer = game:GetService("Players").LocalPlayer
local RaidService = game:GetService("ReplicatedStorage"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("RaidService"):WaitForChild("RF")

RaidService.StartRaid:InvokeServer(LocalPlayer.Name)
RaidService.SpawnMob:InvokeServer("Goblin", -math.huge)
RaidService.EndRaid:InvokeServer()
